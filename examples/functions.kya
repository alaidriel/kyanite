rec Foo {
    x: int,
    y: int,
    z: int
}

fun foo(baz: bool): Foo {
    let bar: int = 5;
    println(bar); % 5
    return Foo:init(
        x: 17,
        y: 500,
        z: 83
    );
}

fun main() {
    let x: Foo = foo(true);
    println(x.y); % 500
    println(x.x); % 17
    println(x.z); % 83
}